; Player information
[Info]
name         = "Gamma-F"   ; Name of character
displayname  = "Gamma-F"  ; Name of character to display
versiondate  = 1.0-A
mugenversion = 1.0           ; Version of M.U.G.E.N. character works on (X.XX)
author       = "[REDACTED], remade by JSSM, from Legal Entity + 1"   ; Character author name
pal.defaults = 1,2,3,4  ; Default palettes in order of preference (up to 4, starts from 1)
localcoord   = 1280,720       ; Local coordinate space (width, height)

; Files for the player
[Files]
;The files part
cmd      = kfm.cmd          ; Command set
cns      = kfm.cns          ; Constants
st       = kfm.cns          ; States
stcommon = common1.cns      ; Common states (from data or motif)
sprite   = Gamma-F.sff      ; Sprite
anim     = Gamma-F.air      ; Animation
sound    = Gamma-F.snd      ; Sound
;ai       = char.ai         ; AI hints data (not used)

;The palette part
pal1 = gammaPalette1.act
pal2 = gammaPalette2.act
pal3 = gammaPalette3.act

; Arcade mode
[Arcade]
;intro.storyboard = intro.def
;ending.storyboard = ending.def
